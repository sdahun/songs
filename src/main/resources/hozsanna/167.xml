<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-04-01T00:00:00">
  <properties>
    <titles>
      <title>Ó, áldd Isten nagy kegyét</title>
      <title lang="en">Give Thanks</title>
    </titles>
    <authors>
      <author type="music">Henry Smith</author>
      <author type="words">Henry Smith</author>
    </authors>
    <ccliNo>20285</ccliNo>
    <verseOrder>v1 v1 c1 c1 e1</verseOrder>
    <songbooks>
      <songbook name="Hozsánna énekes" entry="167"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ó, áldd Isten nagy kegyét,<br/>dicsérd buzgón szent nevét,<br/>és Őt szívedből imádd,<br/>küldte egy Fiát!</lines>
    </verse>
    <verse name="c1">
      <lines>Ó, halld,<br/>Krisztus hozzánk önként jött,<br/>bűnös földre költözött,<br/>ki gazdag volt, szegénnyé lett értünk.</lines>
    </verse>
    <verse name="e1">
      <lines>Őt áldd!</lines>
    </verse>
  </lyrics>
</song>
