<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-04-01T00:00:00">
  <properties>
    <titles>
      <title>Halleluja, zengve zeng hálaének</title>
    </titles>
    <verseOrder>v1 c1</verseOrder>
    <songbooks>
      <songbook name="Hozsánna énekes" entry="87"/>
      <songbook name="Erőm és énekem az Úr" entry="50"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Halleluja,<br/>zengve zeng hálaének, halleluja,<br/>zengve zeng hálaének, halleluja,<br/>Jézus, Te égi bárány!</lines>
    </verse>
    <verse name="c1">
      <lines>Benned lettem én is új teremtés,<br/>Benned megtaláltam életem célját,<br/>Téged dicsér már a szívem és szám.</lines>
    </verse>
  </lyrics>
</song>
