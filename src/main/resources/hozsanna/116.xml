<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-04-01T00:00:00">
  <properties>
    <titles>
      <title>Jó Atyám imádlak</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <songbooks>
      <songbook name="Hozsánna énekes" entry="116"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jó Atyám, imádlak,<br/>leteszem életem,<br/>úgy szeretlek.</lines>
    </verse>
    <verse name="v2">
      <lines>Úr Jézus, imádlak,<br/>leteszem életem,<br/>úgy szeretlek.</lines>
    </verse>
    <verse name="v3">
      <lines>Szent Lélek, úgy várlak,<br/>leteszem életem,<br/>úgy szeretlek.</lines>
    </verse>
  </lyrics>
</song>
