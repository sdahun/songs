<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-04-01T00:00:00">
  <properties>
    <titles>
      <title>Ne félj, ne aggódj</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Hozsánna énekes" entry="162"/>
      <songbook name="Adj zengő éneket!" entry="96"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ne félj, ne aggódj,<br/>ne sírj, ne bánkódj:<br/>ha tiéd Isten tiéd már minden.<br/>Ne félj, ne aggódj,<br/>ne sírj, ne bánkódj:<br/>elég Ő néked.</lines>
    </verse>
  </lyrics>
</song>
