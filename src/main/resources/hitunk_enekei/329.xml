<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2016-08-01T00:00:00">
  <properties>
    <titles>
      <title>Áldja Isten éltünket</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author type="music">Ahle Johann Rudolf, 1664. (1625-1673.)</author>
      <author type="words">Schenck Hartmann, 1674. (1634-1681.)</author>
    </authors>
    <songbooks>
      <songbook name="Hitünk énekei" entry="329"/>
    </songbooks>
    <themes>
      <theme>A gyülekezet élete - záró ének</theme>
      <theme>Záróének</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Áldja Isten éltünket,<br/>és a kimenetelünket,<br/>áldja meg kenyerünket,<br/>minden cselekedetünket;<br/>áldjon csöndes nyugovással,<br/>végül örök boldogsággal!</lines>
    </verse>
  </lyrics>
</song>
