  <properties>
    <titles>
      <title>Téged, Isten, dicsérünk</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author type="music">Wilkes John, 1861.</author>
      <author type="words">Baker Henry (1821-1877.) </author>
    </authors>
