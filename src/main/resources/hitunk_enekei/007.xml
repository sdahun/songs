      <songbook name="Hitünk énekei" entry="7"/>
    </songbooks>
    <themes>
      <theme>Az Atya Isten dicsérete</theme>
      <theme>Gondviselés</theme>
      <theme>Hálaadás</theme>
      <theme>Isten dicsérete</theme>
      <theme>Isten hatalma</theme>
      <theme>Isten igéje</theme>
      <theme>Kegyelem</theme>
      <theme>Lelki táplálék</theme>
