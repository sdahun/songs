<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-02-01T00:00:00">
  <properties>
    <titles>
      <title>Áldásoddal megyünk</title>
    </titles>
    <verseOrder>v1 c1 v2 c1</verseOrder>
    <authors>
      <author type="words">Kádár Ferenc</author>
    </authors>
    <songbooks>
      <songbook name="Erőm és énekem az Úr" entry="3"/>
      <songbook name="Adj zengő éneket!" entry="7"/>
      <songbook name="Dicsérem Neved 2" entry="22"/>
      <songbook name="Hozsánna énekes" entry="16"/>
    </songbooks>
    <themes>
      <theme>Esti ének</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Áldásoddal megyünk,<br/>megyünk innen el,<br/>néked énekelünk<br/>boldog éneket.</lines>
    </verse>
    <verse name="c1">
      <lines>Maradj mindig velünk,<br/>ha útra kelünk,<br/>őrizd életünk<br/>minden nap.</lines>
    </verse>
    <verse name="v2">
      <lines>Minden nap dicsérünk<br/>Téged jó Uram!<br/>Néked éneklünk<br/>vidáman, boldogan.</lines>
    </verse>
  </lyrics>
</song>
