<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2016-08-01T00:00:00">
  <properties>
    <titles>
      <title>Áldjad lelkem az Urat</title>
    </titles>
    <verseOrder>v1 r1 v1</verseOrder>
    <songbooks>
      <songbook name="Zuglói Adventista Gyülekezeti Énekeskönyv" entry="11"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Áldjad lelkem az Urat<br/>és egész bensőm szent nevét!<br/>Áldjad lelkem az Urat<br/>és ne feledd mennyi jót tett veled!</lines>
    </verse>
    <verse name="r1">
      <lines>Ő megbocsátja minden bűnödet,<br/>Ő meggyógyítja betegségedet!</lines>
    </verse>
  </lyrics>
</song>
