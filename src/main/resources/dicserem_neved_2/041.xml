<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2014-04-24T00:00:00">
  <properties>
    <titles>
      <title>Dicső, nagy Király vagy</title>
    </titles>
    <verseOrder>c1 c1 v1 c1 c1</verseOrder>
    <songbooks>
      <songbook name="Dicsérem Neved 2" entry="41"/>
    </songbooks>
      <authors>
      <author type="music">T. Pettygrove</author>
      <author type="translation" lang="hu">Simonfalvi Zs.</author>
    </authors>
    <themes>
      <theme>Dicséret és imádat</theme>
    </themes>
</properties>
  <lyrics>
    <verse name="c1">
      <lines>Dicső, nagy Király vagy,<br/>dicső, nagy Király vagy,<br/>fénybe öltözött, szemet szédítő,<br/>dicső, nagy király vagy!</lines>
    </verse>
    <verse name="v1">
      <lines>Magasztaljuk szépségét,<br/>ünnepeljük Őt,<br/>a mi drága megmentőnk<br/>a nagy nagy Király!</lines>
    </verse>
  </lyrics>
</song>