<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-03-01T00:00:00">
  <properties>
    <titles>
      <title>Aki a holtakat megeleveníti</title>
    </titles>
    <verseOrder>v1 v1 c1 c1</verseOrder>
    <songbooks>
      <songbook name="Dicsérem Neved 2" entry="2"/>
    </songbooks>
    <themes>
      <theme>Bizonyságtétel és megvallás</theme>
      <theme>Dicséret és imádat</theme>
      <theme>Róm 4,17</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Aki a holtakat megeleveníti,<br/>és azokat, amelyek nincsenek,<br/>Előszólítja, mint meglevőket.</lines>
    </verse>
    <verse name="c1">
      <lines>Jézus, Te vagy minden örömöm forrása,<br/>igen, Jézus, Te vagy, Te vagy az élet!</lines>
    </verse>
  </lyrics>
</song>