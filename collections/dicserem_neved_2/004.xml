  <properties>
    <titles>
      <title>A Mester itt van</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <songbooks>
      <songbook name="Dicsérem Neved 2" entry="4"/>
    </songbooks>
    <themes>
      <theme>Bizonyságtétel és megvallás</theme>
      <theme>Zsolt 55,23</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>A Mester itt van, ma is gyógyít,<br/>Jézus gyógyít és szabaddá tesz.<br/>A Mester itt van, ma is gyógyít,<br/>Jézus gyógyít és szabaddá tesz.<br/>Ne vidd tovább a terhedet,<br/>Ő letörli minden könnyedet,<br/>gyászod nagy örömre fordul!</lines>
    </verse>
  </lyrics>
</song>