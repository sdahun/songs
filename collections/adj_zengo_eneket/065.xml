<?xml version='1.0' encoding='utf-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="byHand 1.0" modifiedIn="byHand 1.0" modifiedDate="2018-08-01T00:00:00">
  <properties>
    <titles>
      <title>Jézus, véred megtisztít</title>
      <title>It’s Your Blood</title>
    </titles>
    <verseOrder>v1 v1 e1</verseOrder>
    <authors>
      <author type="music">Michael Christ</author>
      <author type="words">Michael Christ</author>
      <author type="translation" lang="hu">Harkai Tamás</author>
    </authors>
    <songbooks>
      <songbook name="Adj zengő éneket!" entry="65"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jézus, véred megtisztít,<br/>véred ad új életet,<br/>véred tesz ma szabaddá,<br/>értem folyt e drága vér!<br/>Így a lelkem fehér, mint a hó, mint a hó,<br/>Úr Jézus, Te megölt Bárány.</lines>
    </verse>
    <verse name="e1">
      <lines>Úr Jézus, Te megölt Bárány.</lines>
    </verse>
  </lyrics>
</song>
