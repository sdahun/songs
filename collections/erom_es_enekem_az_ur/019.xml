    <verseOrder>v1</verseOrder>
    <authors>
      <author>Edith McNeill</author>
    </authors>
    <songbooks>
      <songbook name="Erőm és énekem az Úr" entry="19"/>
    </songbooks>
    <themes>
      <theme>Reggeli ének</theme>
      <theme>JSir 3,22-23</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Az Úr jósága, hogy még élünk a földön,<br/>mert az Ő irgalma soha véget nem ér:<br/>újjá éled reggelre, minden reggelre.<br/>Nagy a Te hűséged, ó, Urunk, nagy a Te hűséged.</lines>
    </verse>
  </lyrics>
